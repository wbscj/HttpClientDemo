[
  {
    "description": "测试 post 带json请求",
    "request": {
      "uri": "/post",
      "method": "post",
      "json": {
        "name": "weifeng",
        "age": 20
      }
    },
    "response": {
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "json":{
        "message": "post 请求成功!!!",
        "start": 200
      }

    }
  },

  {
    "description": "post请求，forms 表单",
    "request": {
      "uri": "/post/from",
      "method": "post",
      "forms": {
        "name": "weifeng",
        "age": "20",
        "message": "加油"
      }
    },
    "response": {
      "headers": {
        "Content-Type":"text/html;charset=gbk"
      },
      "text": "forms 表单请求成功！"
    }
  }
]